            <xsd:element name="RetailPrice" type="xsd:string" minOccurs="0" nillable="true"/>
            <xsd:element name="UnitSalePrice" type="xsd:decimal" minOccurs="0" nillable="true"/>
            <xsd:element name="DateReceived" type="ns0:dateTime-Timestamp" minOccurs="0" nillable="true"/>
            <xsd:element name="SaleByDate" type="ns0:dateTime-Timestamp" minOccurs="0" nillable="true"/>
            <xsd:element name="Minimum" type="xsd:decimal" minOccurs="0" nillable="true"/>
            <xsd:element name="Maximum" type="xsd:decimal" minOccurs="0" nillable="true"/>
            <xsd:element name="Notesremarks" type="xsd:string" minOccurs="0" nillable="true"/>
            <xsd:element name="Variance" type="xsd:decimal" minOccurs="0" nillable="true"/>
            <xsd:element name="VarianceReason" type="xsd:string" minOccurs="0" nillable="true"/>
        </xsd:sequence>

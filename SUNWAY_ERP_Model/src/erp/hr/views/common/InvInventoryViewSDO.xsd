<xsd:schema xmlns:sdoJava="commonj.sdo/java" xmlns:ns0="http://xmlns.oracle.com/adf/svc/types/" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     targetNamespace="/erp/hr/views/common/" xmlns="/erp/hr/views/common/" elementFormDefault="qualified"
     sdoJava:package="erp.hr.views.common">
    <xsd:import schemaLocation="classpath:/META-INF/wsdl/BC4JService.xsd" namespace="http://xmlns.oracle.com/adf/svc/types/"/>
    <xsd:import schemaLocation="classpath:/xml/sdoJava.xsd" namespace="commonj.sdo/java"/>
    <xsd:complexType name="InvInventoryViewSDO">
        <xsd:annotation>
            <xsd:appinfo source="http://xmlns.oracle.com/adf/svc/metadata/">
                <key xmlns="http://xmlns.oracle.com/adf/svc/metadata/">
                    <attribute>ItemCode</attribute>
                    <attribute>SerialNumber</attribute>
                    <attribute>Location</attribute>
                </key>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ItemCode" type="xsd:string" minOccurs="0"/>
            <xsd:element name="SerialNumber" type="xsd:decimal" minOccurs="0"/>
            <xsd:element name="Location" type="xsd:string" minOccurs="0" nillable="true"/>
